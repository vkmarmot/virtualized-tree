{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "firefox": "40",
          "chrome": "17",
          "ie": "11"
        }
      }
    ],
    "@babel/preset-react",
    ["@babel/preset-typescript", {
      "isTSX": true,
      "allExtensions": true
    }]
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}